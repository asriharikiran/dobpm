      <extensionElements>
        <activiti:in source="JSON_PAYLOAD" target="JSON_PAYLOAD"/>
        <activiti:in source="STAGE_ID" target="STAGE_ID"/>
        <modeler:editor-resource-id><![CDATA[clientRecoveryFlow]]></modeler:editor-resource-id>
      </extensionElements>
    </callActivity>
